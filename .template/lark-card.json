{
  "msg_type": "interactive",
  "card": {
    "elements": [{
      "tag": "div",
      "text": {
        "content": "**Repository**\nName: ${{ github.repository }}\nBranch: ${{ github.ref_name }}",
        "tag": "lark_md"
      }
    },
      {
        "tag": "div",
        "text": {
          "content": "**Event**\n${{ github.event_name }}",
          "tag": "lark_md"
        }
      },
      {
        "tag": "div",
        "text": {
          "content": "**Message**\nCommit ID: ${{ github.event.head_commit.sha }} \nCommit Message: ${{ github.event.head_commit.message }}",
          "tag": "lark_md"
        }
      },
      {
        "tag": "div",
        "text": {
          "content": "**Operator**\n${{ github.actor }}",
          "tag": "lark_md"
        }
      }],
    "header": {
      "title": {
        "content": "Repository-Notify",
        "tag": "plain_text"
      }
    }
